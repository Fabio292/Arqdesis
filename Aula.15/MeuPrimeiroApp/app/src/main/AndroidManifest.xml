<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="www.usjt.br.meuprimeiroapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayMessageActivity" />
        <activity
            android:name=".ListarClientesActivity"
            android:label="@string/title_activity_listar_clientes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DetalheClienteActivity" />
        <activity
            android:name=".AdiconarClienteActivity"
            android:label="@string/title_activity_adiconar_cliente"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>